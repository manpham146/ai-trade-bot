{
  "name": "ai-trade-bot",
  "version": "1.0.0",
  "description": "AI-powered cryptocurrency trading bot for BTC/USDT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "dev:dashboard": "ts-node src/web/server.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:strategy": "ts-node src/test/strategy-test.ts",
    "test:ai-manager": "ts-node scripts/test-ai-manager.ts",
    "test:ai-advisor-toggle": "ts-node scripts/test-ai-advisor-toggle.ts",
    "test:generate-signals": "ts-node scripts/test-generate-signals.ts",
    "test:external-ai": "ts-node src/test/external-ai-test.ts",
    "test:okx": "node test-okx-connection.js",
    "test:all-functions": "ts-node scripts/test-all-functions.ts",
    "demo": "node demo.js",
    "demo:external-ai": "node demo-external-ai.js",
    "setup": "echo 'Copying .env.example to .env...' && cp .env.example .env && echo 'Please edit .env file with your API keys'",
    "setup:wizard": "ts-node src/setup-wizard.ts",
    "tools:backtest": "ts-node src/backtest/backtest.ts",
    "tools:health-check": "ts-node src/utils/healthCheck.ts",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "maintenance:update-deps": "npm update"
  },
  "keywords": [
    "trading",
    "bot",
    "cryptocurrency",
    "ai",
    "bitcoin",
    "binance"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.6.0",
    "ccxt": "^4.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.1.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
